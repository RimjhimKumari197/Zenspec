<script>
    const fc = document.querySelector("#full-container");
const header=document.querySelector(".header");
window.addEventListener("load", () => {
  
  requestAnimationFrame(() => {
    requestAnimationFrame(() => {
      gsap.from(fc,{
        y: "10vh",
        duration: 1.5,
        ease: "elastic.out(1, 0.5)",
        onComplete: () => {
          gsap.set(fc, { clearProps: "transform" });
         
        },
      });
    });
  });
   });
</script>
